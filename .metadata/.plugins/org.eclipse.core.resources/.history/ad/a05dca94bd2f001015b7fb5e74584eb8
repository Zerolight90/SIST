package pm;

import java.util.Scanner;

public class Ex3_Gugudan {
	
	private int dan;
	
	
	public int getDan() {
		return dan;
	}

	public void setDan(int dan) {
		this.dan = dan;
		Scanner scan = new Scanner(System.in);
		System.out.println("단을 입력해주세요");
		int i = scan.nextInt();
		
		
		for(int k=2; k<10; k++) {
			
			for(int j=1; 1<10; j++) {
				int num = i*j;
				System.out.printf("%-3d, %-3d, %-3d", i+"x"+"j"+"="+k);
			}
		}
		
		
		
	}
	
	
	
	
	
	
	
}

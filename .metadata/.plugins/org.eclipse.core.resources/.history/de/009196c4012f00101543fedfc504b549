package pm;

public class Ex7_Array_Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] ar1 = {"다이아","금두꺼비","꽝","로또1등"};
		String[] ar2 = {"꽝","황금알","비누","아파트"};
		String[] ar3 = {"꽝","천원","꽝","거품"};
		
		String[][] ar4 =new String [3][4];
		
		ar4[0] = ar1;
		ar4[1] = ar2;
		ar4[2] = ar3;
		
		for(int i =0; i<ar4.length; i++) {
			for(int j=0; j<ar4[i].length; j++){
				
				
				System.out.printf("%-5s",ar4[i][j]);
			}
			System.out.println(); //줄 바꿈
		}
		

	}

}

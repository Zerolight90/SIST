package am;

public class Ex2_Pet {
	
	private String name; //이름
	private int age;
	
	//펫 이름 영역
	public void setName(String patname) {//인자인 patname을 받아서 뱀버필드 name에 저장하는 기능
		name = patname; //
		
	}
	
	public String getName() {
		return name; //private String name으로 반환
	}
	
	
	//펫 나이 영역
	public void setAge(int petage) {
		age = petage;
	}
	
	public int getAge() {
		return age; // private int age로 반환
	}
	

}

package pm;

public class Ex9_test_Lotto {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//정수 6개를 담는 배열 생성
		int[] num = new int[6];
		
		//난수를 발생시켜 배열채우기
			
		
		for(int i = 0; i<num.length; ++i) {
					
					num [i] = (int)(Math.random()*45+1);
					int j = num[i];
					
				// I 난수 발생
					
				for(int k = 0; k<i; k++) {	
				
					if(num[i]==num[k]) {
						i--;
						break; //반복문 탈출
					}
					
				}				
				
				System.out.printf("%-3d",j);
		}

	}
}



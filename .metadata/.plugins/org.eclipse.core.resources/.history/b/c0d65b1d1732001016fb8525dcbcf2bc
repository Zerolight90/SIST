package pm;

import java.util.HashMap;

public class Ex5_Map {

	public static void main(String[] args) {
		// Map구조 : 키와 값을 하나의 쌍으로 저장하는 구조
		// key들을 중복불가!!!
		
		HashMap<Integer, String> map = new HashMap<Integer, String>();
		
		String s1 = new String("SiST");
		String s2 = "SiST";
		String s3 = "쌍용교육센터";
		String s4 = "SiST";
		
		//Map구조에 저장하자
		
		map.put(100, s1);
		map.put(200, s4);
		map.put(300, s3);
		map.put(400, s2);
		map.put(300, s2); //변경(덮어쓰기)
		
		System.out.printf("map.size():%d",map.size());
		
	}

}

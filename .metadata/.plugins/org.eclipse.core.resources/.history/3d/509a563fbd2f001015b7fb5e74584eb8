package pm;

public class Ex3_Gugudan {
	
	private int dan;

	public int getDan() {
		return dan;
	}

	public void setDan(int dan) {
		this.dan = dan;
		
		for(int i=2; i<10; i++) {
			
			for(int j=1; 1<10; j++) {
				int k = i*j;
				System.out.printf("%-3d, %-3d, %-3d", i+"x"+"j"+"="+k);
			}
		}
		
		
		
	}
	
	
	
	
	
	
	
}
